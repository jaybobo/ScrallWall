<%= image_tag @picture.image.preview.url %>
<br />
<% @canonical_pictures.each do |pic| %>
  <%= link_to (image_tag pic.image.thumb.url), artifact_picture_path(pic.artifact, @picture), method: :patch %>
<% end %>
<br />

<%= form_for @picture, method: :patch do |f| %>
    <%= f.text_field :latitude, value: @latitude, type: 'hidden' %>
    <%= f.text_field :longitude, value: @longitude, type: 'hidden' %>
    <%= f.submit 'create new artifact' %>
<% end %>
